        }
      
        h1 {
          text-align: center;
          color: #333;
        }
      
        p {
          text-align: center;
          color: #666;
          font-size: 18px;
          line-height: 1.5;
        }
      
        .btnSucces {
          display: block;
          width: 200px;
          margin: 20px auto;
          padding: 10px;
          text-align: center;
          color: #fff;
          background-color: #e299a9;
          border: none;
          border-radius: 5px;
          text-decoration: none;
          transition: background-color 0.3s ease;
        }
      
        .btn:hover {
          background-color:#cf7e90;
          color: #fff;
        }
      
        @media screen and (max-width: 600px) {
          .container1 {
            padding: 10px;
          }
      
          h1 {
            font-size: 24px;
          }
      
          p {
