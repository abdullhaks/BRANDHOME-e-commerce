    <!-- Shop Start -->
    <div class="main-content pt-5">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row  align-items-center justify-content-center">
                    <div  class="col-lg-2" > </div>
                    <div class="col-lg-8 row ">
                        
                            <div class="col-lg-4">
                                
                            </div>
                            <div class="col-lg-8 " >
                                <img src="/productImages/<%= orderItem.productImage%>" alt="" style="width: 150px;">
                            </div>


                            <div class="col-lg-4">
                                <p>Product Name </p>
                            </div>
                            <div class="col-lg-8 ">
                                <p class="text-dark"><%= orderItem.productBrand %> <%= orderItem.productName %> ,<%= orderItem.size %>,<%= orderItem.color %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Quantity</p>
                            </div>
                            <div class="col-lg-8 ">
                                <p class="text-dark"><%= orderItem.quantity %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Price</p>
                            </div>
                            <div class="col-lg-8 ">
                                <p class="text-dark"><%= orderItem.price %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Order Date </p>
                            </div>
                            <div class="col-lg-8 ">
                                <p><%= orderItem.orderDate.toLocaleDateString() %></p>
                            </div>



                            <div class="col-lg-4">
                                <p>Order Id </p>
                            </div>
                            <div class="col-lg-8 ">
                                <p><%= order.orderId %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Shipping Charge</p>
                            </div>
                            <div class="col-lg-8 ">
                                <p>Rs <%= orderItem.shippingCharge %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Discount</p>
                            </div>
                            <div class="col-lg-8 ">
                                <p>Rs <%= orderItem.youSave %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Total Amount </p>
                            </div>
                            <div class="col-lg-8 ">
                                <p>Rs <%= orderItem.payAmount %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Shipping Address </p>
                            </div>
                            <div class="col-lg-8 ">
                                <p><%= order.address.firstName %> <%= order.address.lastName %> <br>
                                    <%= order.address.shippingEmail %> <br>
                                    <%= order.address.mobile %> <br>
                                    <%= order.address.street %> ,<%= order.address.city %> <br>
                                    <%= order.address.state %> ,<%= order.address.country %> <br>
                                    pin : <%= order.address.postalCode %>
                                    
                                    
                                    </p>
                            </div>

                            <div class="col-lg-4">
                                <p>Payment Option </p>
                            </div>
                            <div class="col-lg-8 ">
                                <p><%= order.paymentOption %></p>
                            </div>

                            <div class="col-lg-4">
                                <p>Order Status  </p>
                            </div>
                            <div class="col-lg-8 ">

                                <% if( orderItem.status=="dispatched"){ %>
                                    <p class="text-primary"><%= orderItem.status %> - <%= orderItem.dispatchedDate.toLocaleDateString() %> </p> 
                                <% } %>

                                <% if( orderItem.status=="delivered"){ %>
                                    <p class="text-success"><%= orderItem.status %> - <%= orderItem.deliveredDate.toLocaleDateString() %> %></p> 
                                <% } %>

                                <% if( orderItem.status=="cancelled"){ %>
                                    <p class="text-danger"><%= orderItem.status %> - <%= orderItem.cancelDate.toLocaleDateString() %> %></p> 
                                <% } %>

                                <% if( orderItem.status=="pending"){ %>
                                    <p>pending</p> 
                                <% } %>
                            
                                
                            </div>

                         
                    </div>
                    
                    
                </div>
               
                
                
                
            </div>
        </div>
    </div>
